<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/fw_radius"/>
            <solid android:color="@color/fw_transparent_black_33" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/fw_radius"/>
            <solid android:color="@color/fw_transparent" />
        </shape>
    </item>
</selector>